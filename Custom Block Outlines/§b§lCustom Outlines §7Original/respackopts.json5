{
    id: "outlines",
    version: 13,
    capabilities: ["FileFilter", "DirFilter"],
    conf: {
        red: {
            type: "integer",
            min: 0,
            default: 255,
            max: 255
        },
        green: {
            type: "integer",
            min: 0,
            default: 0,
            max: 255
        },
        blue: {
            type: "integer",
            min: 0,
            default: 255,
            max: 255
        },
        alpha: {
            type: "number",
            min: 0.0,
            default: 0.75,
            max: 1.0
        },
        width: {
            type: "integer",
            min: 0,
            default: 2,
            max: 10
        }
    }
}
